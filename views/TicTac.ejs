<!doctype html>

<html lang="en">
<head>

    <title>Tic Tac Toe</title><!--Title of each individual page-->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bxslider/4.2.15/jquery.bxslider.min.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bxslider/4.2.15/jquery.bxslider.min.js"></script>
    <link rel='stylesheet' type="text/css" href='../assets/index.css' />


</head>

<body>
<main>

    <div id="content">
        <%- include('../partials/Gamenav.ejs') %>

        <table class="center">
            <tr>
                <th>
                    <h2 class="information">Score: </h2>
                </th>
            </tr>
            <tr>
                <td>
                    <h2 class="information">Attempt: </h2>
                </td>
            </tr>
        </table>
        <div class="play-area">
            <div id="block_0" class="block"></div>
            <div id="block_1" class="block"></div>
            <div id="block_2" class="block"></div>
            <div id="block_3" class="block"></div>
            <div id="block_4" class="block"></div>
            <div id="block_5" class="block"></div>
            <div id="block_6" class="block"></div>
            <div id="block_7" class="block"></div>
            <div id="block_8" class="block"></div>
        </div>
    </div>

</main>
</body>
</html>